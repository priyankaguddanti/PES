        <servlet-class>pes.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>pes.Logout</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>DummyQues</servlet-name>
        <servlet-class>pes.DummyQues</servlet-class>
